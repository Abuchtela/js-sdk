    "nx": "^14.5.10",
    "nx-plugin-devkit": "^1.3.1",
    "prettier": "^2.6.2",
    "react-test-renderer": "18.2.0",
    "ts-jest": "27.1.4",
    "ts-node": "10.9.1",
    "typedoc": "^0.23.10",
    "typescript": "~4.7.2",
    "cypress-metamask-v2": "1.7.1"
