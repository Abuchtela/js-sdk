{
  "name": "@litprotocol-dev/sdk",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "workspace:reset:all": "sh ./tools/scripts/reset.sh",
    "workspace:reset:dist": "rm -rf ./dist",
    "workspace:reset:cache": "rm -rf ./node_modules/.cache",
    "build:packages": "nx run-many --target=build",
    "test:packages": "nx run-many --target=testPackage",
    "publish:packages:forceDepVersion": "node ./tools/scripts/forceDepVersion.mjs --contains=@litprotocol-dev --workspace=lib --version=*",
    "publish:packages": "npx lerna publish --yes && yarn publish:packages:forceDepVersion",
    "gitAdd": "git add *",
    "gitCommit": "git commit -m 'test: publish'",
    "gitPush": "git push"
  },
  "private": true,
  "dependencies": {
    "@websaam/nx-esbuild": "^0.0.1",
    "core-js": "^3.6.5",
    "g": "^2.0.1",
    "next": "12.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "regenerator-runtime": "0.13.7",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@nrwl/cypress": "14.5.10",
    "@nrwl/eslint-plugin-nx": "14.5.10",
    "@nrwl/jest": "14.5.10",
    "@nrwl/js": "14.5.10",
    "@nrwl/linter": "14.5.10",
    "@nrwl/next": "14.5.10",
    "@nrwl/node": "14.5.10",
    "@nrwl/nx-cloud": "14.4.1",
    "@nrwl/react": "14.5.10",
    "@nrwl/web": "14.5.10",
    "@swc/core": "^1.2.241",
    "@testing-library/react": "13.3.0",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "27.5.1",
    "cypress": "^10.2.0",
    "esbuild": ">0.11.0",
    "esbuild-node-builtins": "^0.1.0",
    "eslint": "~8.15.0",
    "eslint-config-next": "12.2.3",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
