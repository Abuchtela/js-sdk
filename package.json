{
  "name": "@litprotocol-dev/sdk",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "workspace:reset:all": "sh ./tools/scripts/reset.sh",
    "workspace:reset:dist": "rm -rf ./dist",
    "workspace:reset:cache": "rm -rf ./node_modules/.cache",
    "build:packages:forceDepVersion": "node ./tools/scripts/forceDepVersion.mjs --contains=@litprotocol-dev --workspace=lib --version=*",
    "postBuild:mapDistFolderNameToPackageJson": "node ./tools/scripts/map-dist-folder-name-to-package-json.mjs",
    "postBuild:mapDepsToDist": "node tools/scripts/map-deps-to-dist.mjs packages dist @litprotocol-dev",
    "build:packages": "nx run-many --target=build --exclude=e2e,example-nextjs-js,html,nodejs,react && yarn postBuild:mapDepsToDist",
    "build:target": "yarn node tools/scripts/build.mjs",
    "test:packages": "nx run-many --target=testPackage",
    "test:watch": "nx run-many --target=testPackage --watch",
    "publish:packages": "npx lerna publish --force-publish && yarn publish:vanilla",
    "publish:vanilla": "yarn node ./tools/scripts/pub.mjs --filter vanilla",
    "tool:delete": "yarn node ./tools/scripts/delete.mjs",
    "tool:createTest": "yarn node ./tools/scripts/create-case.mjs",
    "tool:genHtml": "yarn node ./tools/scripts/gen-html.mjs",
    "tool:genNodejs": "yarn node tools/scripts/gen-nodejs.mjs",
    "tool:genLib": "yarn node tools/scripts/gen-lib.mjs",
    "tool:findImported": "yarn node tools/scripts/find-imported-packages.mjs",
    "graph": "nx graph",
    "npm:login": "npm login",
    "cy:open": "cp tsconfig.base.json tsconfig.json && CYPRESS_REMOTE_DEBUGGING_PORT=9222 cypress open",
    "cy:open2": "cp tsconfig.base.json tsconfig.json && CYPRESS_REMOTE_DEBUGGING_PORT=9222 cypress run --browser chrome --headed --no-exit"
  },
  "private": true,
  "dependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@cypress/react": "^6.2.0",
    "@cypress/webpack-dev-server": "^2.3.0",
    "@playwright/test": "^1.25.2",
    "@synthetixio/js": "^2.41.0",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/react": "^13.4.0",
    "@types/testing-library__cypress": "^5.0.9",
    "@walletconnect/ethereum-provider": "^2.1.2",
    "@websaam/nx-esbuild": "^0.0.1",
    "axios": "^0.27.2",
    "bytes32": "^0.0.3",
    "commander": "^9.4.0",
    "concurrently": "^7.4.0",
    "core-js": "^3.6.5",
    "cypress-wait-until": "^1.7.2",
    "cypress-watch-and-reload": "^1.10.3",
    "dotenv": "^16.0.2",
    "dotenv-parse-variables": "^2.0.0",
    "download": "^8.0.0",
    "ethers": "^5.7.1",
    "etherscan-api": "^10.2.0",
    "find-config": "^1.0.0",
    "g": "^2.0.1",
    "jszip": "^3.10.1",
    "lit-connect-modal": "0.1.8",
    "lit-siwe": "^1.1.8",
    "next": "12.2.3",
    "node-fetch": "^2.6.1",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "regenerator-runtime": "0.13.7",
    "serve": "^14.0.1",
    "start-server-and-test": "^1.14.0",
    "synthetix-js": "^2.74.1",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@nrwl/cypress": "14.5.10",
    "@nrwl/eslint-plugin-nx": "14.5.10",
    "@nrwl/jest": "14.5.10",
    "@nrwl/js": "14.5.10",
    "@nrwl/linter": "14.5.10",
    "@nrwl/next": "14.5.10",
    "@nrwl/node": "14.5.10",
    "@nrwl/nx-cloud": "14.4.1",
    "@nrwl/nx-plugin": "^15.0.13",
    "@nrwl/react": "14.5.10",
    "@nrwl/web": "14.5.10",
    "@swc/core": "^1.2.241",
    "@synthetixio/synpress": "^2.3.3",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "27.5.1",
    "cypress": "11.0.1",
    "cypress-metamask": "^1.0.5-development",
    "cypress-metamask-v2": "^1.7.2",
    "esbuild": ">0.11.0",
    "esbuild-node-builtins": "^0.1.0",
    "eslint": "~8.15.0",
    "eslint-config-next": "12.2.3",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "27.5.1",
    "lerna": "^5.4.3",
    "live-server": "^1.2.2",
    "nodemon": "^2.0.20",
    "nx": "^14.5.10",
    "nx-plugin-devkit": "^1.3.1",
    "path": "^0.12.7",
    "prettier": "^2.6.2",
    "react-test-renderer": "18.0.0",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "27.1.4",
    "ts-node": "10.9.1",
    "typedoc": "^0.23.10",
    "typescript": "~4.7.2"
  },
  "workspaces": [
    "packages/*"
  ]
}
