  "scripts": {
    "workspace:reset:all": "sh ./tools/scripts/reset.sh",
    "workspace:reset:dist": "rm -rf ./dist",
    "workspace:reset:cache": "rm -rf ./node_modules/.cache",
    "build:packages": "nx run-many --target=build",
    "test:packages": "nx run-many --target=testPackage",
    "lerna:bootstrap": "lerna bootstrap -- -- --legacy-peer-deps",
    "publish:packages:forceDepVersion": "node ./tools/scripts/forceDepVersion.mjs --contains=@litprotocol-dev --workspace=lib --version=*",
    "publish:packages": "npx lerna publish --yes && yarn publish:packages:forceDepVersion",
    "gitAdd": "git add *",
    "gitCommit": "git commit -m 'test: publish'",
    "gitPush": "git push"
  },
  "private": true,
