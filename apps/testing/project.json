{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/testing/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "apps/testing/cypress.config.ts",
        "devServerTarget": "example-nextjs-js:serve:development",
        "testingType": "e2e",
        "watch": true,
        "browser": "chrome"
      },
      "configurations": {
        "production": {
          "devServerTarget": "example-nextjs-js:serve:production"
        }
      }
    },
    "testing":{
      "executor": "nx:run-commands",
      "options": {
        "command": "CYPRESS_REMOTE_DEBUGGING_PORT=9222 yarn cypress open"
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["example-nextjs-js"]
}
