{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/example-html/src",
  "tags": [],
  "targets": {
    "buildApp": {
      "executor": "@nrwl/web:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/example-html",
        "compiler": "babel",
        "index": "apps/example-html/src/index.html",
        "baseHref": "/",
        "main": "apps/example-html/src/main.ts",
        "polyfills": "apps/example-html/src/polyfills.ts",
        "tsConfig": "apps/example-html/tsconfig.app.json",
        "assets": [
          "apps/example-html/src/favicon.ico",
          "apps/example-html/src/assets",
          "apps/example-html/src/dist/constants-vanilla"
        ],
        "styles": ["apps/example-html/src/styles.css"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/example-html/src/environments/environment.ts",
              "with": "apps/example-html/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "cloneDist": {
      "executor": "nx:run-commands",
      "options": {
        "command": "mkdir -p ./apps/example-html/src/dist && cp -r ./dist/packages/constants-vanilla ./apps/example-html/src/dist/constants-vanilla"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn nx run example-html:cloneDist && yarn nx run example-html:serve"
      }
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "options": {
        "buildTarget": "example-html:buildApp"
      },
      "configurations": {
        "production": {
          "buildTarget": "example-html:buildApp:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/example-html/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/example-html"],
      "options": {
        "jestConfig": "apps/example-html/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
