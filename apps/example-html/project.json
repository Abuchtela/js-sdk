{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/example-html/src",
  "tags": [],
  "targets": {
    "buildApp": {
      "executor": "@nrwl/web:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/example-html",
        "compiler": "babel",
        "index": "apps/example-html/src/index.html",
        "baseHref": "/",
        "main": "apps/example-html/src/main.ts",
        "polyfills": "apps/example-html/src/polyfills.ts",
        "tsConfig": "apps/example-html/tsconfig.app.json",
        "assets": [
          "apps/example-html/src/favicon.ico",
          "apps/example-html/src/assets",
          {
            "glob": "**/*",
            "input": "dist/packages/constants",
            "output": "./constants"
          }
        ],
        "styles": ["apps/example-html/src/styles.css"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/example-html/src/environments/environment.ts",
              "with": "apps/example-html/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "options": {
        "buildTarget": "..-apps-example-html:build",
        "port": 4000
      },
      "configurations": {
        "production": {
          "buildTarget": "..-apps-example-html:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/example-html/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/example-html"],
      "options": {
        "jestConfig": "apps/example-html/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
